{
  "name": "svelte-todo",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build --base=/ui5-webcomponents-sample-svelte/",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint -e",
    "prettier": "prettier --write .",
    "prettier:staged": "pretty-quick --staged --verbose",
    "prepare": "node ./.husky/skip.js || husky install",
    "hooks:pre-commit": "run-s prettier:staged lint:staged",
    "hooks:pre-push": "run-s lint:commit",
    "ui5:build:svelte": "vite build --mode UI5",
    "ui5:copy": "shx cp ./distUI5/sveltetodo.umd.cjs ./webapp/sveltetodo.js && cp ./distUI5/style.css ./webapp/sveltestyle.css",
    "ui5:build": "ui5 build --clean-dest",
    "ui5:start": "ui5 serve --port 8080 -o index.html",
    "ui5:start:lp": "ui5 serve --port 8080 -o launchpad.html",
    "ui5:run:start": "run-s ui5:build:svelte ui5:copy ui5:start",
    "ui5:run:start:lp": "run-s ui5:build:svelte ui5:copy ui5:start:lp",
    "ui5:run:build": "run-s ui5:build:svelte ui5:copy ui5:build"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@prettier/plugin-xml": "^2.2.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@sapui5/ts-types-esm": "1.118.1",
    "@sveltejs/vite-plugin-svelte": "latest",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@ui5/cli": "^3.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.27.0",
    "eslint-plugin-svelte": "^2.33.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prettier-plugin-properties": "^0.1.0",
    "prettier-plugin-svelte": "^2.7.0",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "svelte": "^4.0.0",
    "svelte-check": "^3.0.0",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "ui5-middleware-livereload": "^0.7.0",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "@ui5/webcomponents": "^1.4.0",
    "@ui5/webcomponents-base": "^1.4.0",
    "@ui5/webcomponents-fiori": "^1.4.0",
    "@ui5/webcomponents-icons": "^1.4.0",
    "rollup-plugin-styles": "^4.0.0"
  }
}
